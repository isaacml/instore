Global config_shop

Global Entidades, Almacenes, Paises, Regiones, Provincias, Tiendas, Enviar


Procedure Openconfig_shop(x = 0, y = 0, width = 600, height = 400)
  config_shop = OpenWindow(#PB_Any, x, y, width, height, "Configuración de la Tienda", #PB_Window_SystemMenu)
  Entidades = ComboBoxGadget(#PB_Any, 230, 80, 160, 25)
  Almacenes = ComboBoxGadget(#PB_Any, 230, 120, 160, 25)
  Paises = ComboBoxGadget(#PB_Any, 230, 160, 160, 25)
  Regiones = ComboBoxGadget(#PB_Any, 230, 200, 160, 25)
  Provincias = ComboBoxGadget(#PB_Any, 230, 240, 160, 25)
  Tiendas = ComboBoxGadget(#PB_Any, 230, 280, 160, 25)
  Enviar = ButtonGadget(#PB_Any, 260, 330, 100, 25, "Enviar")
  DisableGadget(Enviar, 1)
EndProcedure

Procedure config_shop_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

