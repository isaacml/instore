LoadFont(#Font_menu_0,"Verdana", 12)


Procedure Openmenu(x = 0, y = 0, width = 800, height = 600)
  menu = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_SystemMenu | #PB_Window_ScreenCentered | #PB_Window_WindowCentered)
  CreateToolBar(0, WindowID(menu))
  ToolBarImageButton(#logout_menu,ImageID(Img_menu_0))
  ToolBarToolTip(0, #logout_menu, "Desconectar")
  prog_mu = ButtonGadget(#PB_Any, 320, ToolBarHeight(0) + 130, 180, 30, "Programar Música")
  SetGadgetColor(prog_mu, #PB_Gadget_BackColor,RGB(255,0,0))
  SetGadgetFont(prog_mu, FontID(#Font_menu_0))
  msg_normal = ButtonGadget(#PB_Any, 320, ToolBarHeight(0) + 190, 180, 30, "Mensajes")
  SetGadgetColor(msg_normal, #PB_Gadget_BackColor,RGB(255,0,0))
  SetGadgetFont(msg_normal, FontID(#Font_menu_0))
  doms = ButtonGadget(#PB_Any, 320, ToolBarHeight(0) + 250, 180, 30, "Dominios")
  SetGadgetColor(doms, #PB_Gadget_BackColor,RGB(255,0,0))
  SetGadgetFont(doms, FontID(#Font_menu_0))
  shop_status = ButtonGadget(#PB_Any, 320, ToolBarHeight(0) + 310, 180, 30, "Tienda")
  SetGadgetColor(shop_status, #PB_Gadget_BackColor,RGB(255,0,0))
  SetGadgetFont(shop_status, FontID(#Font_menu_0))
EndProcedure

Procedure menu_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
        Case #logout_menu
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure


