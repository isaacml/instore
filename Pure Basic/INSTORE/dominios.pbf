Global dominios

Global lbl_dom1, Entidades, Almacenes, Paises, Regiones, Provincias, Tiendas, enviar_dom, lista_dominios, lbl_dom2, err_dom, reconfigurar

Enumeration FormMenu
  #prog_dom
  #msg_dom
  #dom_dom
  #shop_dom
EndEnumeration

Enumeration FormFont
  #Font_dominios_0
  #Font_dominios_1
EndEnumeration

LoadFont(#Font_dominios_0,"Verdana", 14)
LoadFont(#Font_dominios_1,"Verdana", 12)


Procedure Opendominios(x = 0, y = 0, width = 800, height = 600)
  dominios = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_SystemMenu | #PB_Window_ScreenCentered | #PB_Window_WindowCentered)
  CreateMenu(0, WindowID(dominios))
  MenuTitle("Menu")
  MenuItem(#prog_dom, "Programar Música")
  MenuItem(#msg_dom, "Mensajes")
  MenuItem(#dom_dom, "Dominios")
  MenuItem(#shop_dom, "Tienda")
  lbl_dom1 = TextGadget(#PB_Any, 280, 30, 280, 30, "Añadir dominios adicionales")
  SetGadgetColor(lbl_dom1, #PB_Gadget_FrontColor,RGB(0,0,64))
  SetGadgetFont(lbl_dom1, FontID(#Font_dominios_0))
  Entidades = ComboBoxGadget(#PB_Any, 320, 70, 190, 25)
  Almacenes = ComboBoxGadget(#PB_Any, 320, 110, 190, 25)
  Paises = ComboBoxGadget(#PB_Any, 320, 150, 190, 25)
  Regiones = ComboBoxGadget(#PB_Any, 320, 190, 190, 25)
  Provincias = ComboBoxGadget(#PB_Any, 320, 230, 190, 25)
  Tiendas = ComboBoxGadget(#PB_Any, 320, 270, 190, 25)
  enviar_dom = ButtonGadget(#PB_Any, 370, 310, 90, 25, "Enviar")
  SetGadgetFont(enviar_dom, FontID(#Font_dominios_1))
  DisableGadget(enviar_dom, 1)
  lista_dominios = ListViewGadget(#PB_Any, 180, 430, 470, 80)
  lbl_dom2 = TextGadget(#PB_Any, 320, 390, 190, 30, "Dominios Actuales")
  SetGadgetColor(lbl_dom2, #PB_Gadget_FrontColor,RGB(0,0,64))
  SetGadgetFont(lbl_dom2, FontID(#Font_dominios_0))
  err_dom = TextGadget(#PB_Any, 180, 350, 470, 25, "", #PB_Text_Center)
  SetGadgetColor(err_dom, #PB_Gadget_FrontColor,RGB(255,0,0))
  reconfigurar = ButtonGadget(#PB_Any, 350, 520, 120, 25, "Reconfigurar")
  SetGadgetFont(reconfigurar, FontID(#Font_dominios_1))
EndProcedure

Procedure dominios_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
        Case #prog_dom
        Case #msg_dom
        Case #dom_dom
        Case #shop_dom
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

