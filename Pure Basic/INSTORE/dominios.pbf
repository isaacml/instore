Global dominios

Global lbl_dom1, Entidades

Global Img_dominios_0, Img_dominios_1

Enumeration FormMenu
  #back_dom
  #logout_dom
EndEnumeration

UsePNGImageDecoder()

Img_dominios_0 = LoadImage(#PB_Any,"C:\Users\Isaac\Documents\PB\INSTORE\logout.png")
Img_dominios_1 = LoadImage(#PB_Any,"C:\Users\Isaac\Documents\PB\INSTORE\flecha-atras.png")

Enumeration FormFont
  #Font_dominios_0
EndEnumeration

LoadFont(#Font_dominios_0,"Verdana", 14)


Procedure Opendominios(x = 0, y = 0, width = 800, height = 600)
  dominios = OpenWindow(#PB_Any, x, y, width, height, "", #PB_Window_SystemMenu | #PB_Window_ScreenCentered | #PB_Window_WindowCentered)
  CreateToolbar(0, WindowID(dominios))
  ToolBarImageButton(#back_dom,ImageID(Img_dominios_1))
  ToolBarToolTip(0, #back_dom, "Atrás")
  ToolBarSeparator()
  ToolBarImageButton(#logout_dom,ImageID(Img_dominios_0))
  ToolBarToolTip(0, #logout_dom, "Desconectar")
  lbl_dom1 = TextGadget(#PB_Any, 290, ToolBarHeight(0) + 60, 280, 30, "Añadir dominios adicionales")
  SetGadgetColor(lbl_dom1, #PB_Gadget_FrontColor,RGB(0,0,64))
  SetGadgetFont(lbl_dom1, FontID(#Font_dominios_0))
  Entidades = ComboBoxGadget(#PB_Any, 330, ToolBarHeight(0) + 100, 200, 25)
EndProcedure

Procedure dominios_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
        Case #back_dom
        Case #logout_dom
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

