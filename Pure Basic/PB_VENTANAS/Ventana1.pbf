Global Admin

Global Cerrar, Abrir


Procedure OpenAdmin(x = 0, y = 0, width = 600, height = 400)
  Admin = OpenWindow(#PB_Any, x, y, width, height, "Panel Administrador", #PB_Window_SystemMenu | #PB_Window_MinimizeGadget)
  Cerrar = ButtonGadget(#PB_Any, 460, 350, 100, 25, "Cerrar")
  Abrir = ButtonGadget(#PB_Any, 40, 350, 100, 25, "Abrir")
EndProcedure

Procedure Admin_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

